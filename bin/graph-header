#!/bin/sh

FILENAME=$1
FORMAT=$2
TITLE=$3
CUSTOM=$4

cat <<EOF
set terminal png size 900,600
set xdata time
set timefmt "%s"
set output "$FILENAME"
set grid
set xlabel "Time"
set format x "%d-%H:%M"
set xtic rotate by -45 scale 0
set ylabel ""
set ytics format "$FORMAT" nomirror
set title "$TITLE"
set key top right box
$CUSTOM
plot \\
EOF
